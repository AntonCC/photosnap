        }\n      </Switch>\n      <Footer />\n    </div>\n  );\n}\n\nexport default App;\n","/home/ant/Documents/WebProjects/photosnap/src/components/square-btn/square-btn.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/navbar/navbar.jsx",["144","145"],"/home/ant/Documents/WebProjects/photosnap/src/pages/home/home.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/banner/banner.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/button/button.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/cards/cards.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/card/card.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/gems/gems.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/gem/gem.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/footer/footer.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/stories/stories.jsx",["146"],"/home/ant/Documents/WebProjects/photosnap/src/components/featured-card/featured-card.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/home/home-info.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/stories/stories-info.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/features/features.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/features/features-info.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/full-img-banner/full-img-banner.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/pricing/pricing.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/pages/pricing/pricing-info.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/prices/prices.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/price-card/price-card.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/prices-switch/prices-switch.jsx",[],"/home/ant/Documents/WebProjects/photosnap/src/components/table/table.jsx",[],"/home/ant/Documents